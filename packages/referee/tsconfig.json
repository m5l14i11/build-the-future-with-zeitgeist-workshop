{
  "include": ["./src"],
  "exclude": ["dist","node_modules"],
  "compilerOptions": {
    "strict": true,
    "target": "ES2019",
    "module": "CommonJS",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "isolatedModules": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "outDir": "./dist",
  }
}
